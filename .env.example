# API Keys and Configuration
# Copy this file to .env and fill in your actual keys

# ElevenLabs API
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_API_URL=https://api.elevenlabs.io/v1

# Ngrok Token
NGROK_TOKEN=your_ngrok_token_here

# n8n MCP Server
N8N_MCP_URL=https://your-n8n-instance.app.n8n.cloud/mcp-server/http
N8N_MCP_JWT=your_n8n_jwt_token_here

# Recall.ai
RECALL_API_TOKEN=your_recall_api_token_here

# Anam.ai
ANAM_API_KEY=your_anam_api_key_here

# Optional: n8n Local Webhook
N8N_WEBHOOK_URL=http://localhost:5678/webhook/interview-webhook

# Optional: Meeting URL
MEETING_URL=https://meet.google.com/your-meeting-id
